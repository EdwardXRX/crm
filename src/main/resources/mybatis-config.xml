<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>


    <!--打印日志
    <settings>
        <setting name="logImpl" value="STDOUT_LOGGING"/>
    </settings>-->

    <!--设置别名-->
    <typeAliases>
        <package name="com.edward.crm_ssh.settings.domain"></package>
        <package name="com.edward.crm_ssh.workbench.domain"></package>
    </typeAliases>

    <!--sql mapper(sql映射文件)的位置-->
    <mappers>
        <package name="com.edward.crm_ssh.settings.dao"/>
        <package name="com.edward.crm_ssh.workbench.dao"/>
    </mappers>
</configuration>